{
  "name": "electron_ups_easy_client",
  "productName": "Easy UPS Client",
  "version": "1.0.0",
  "description": "Desktop client for monitoring UPS systems via NUT protocol",
  "main": "./.vite/build/main.js",
  "private": true,
  "scripts": {
    "start": "electron-forge start",
    "start:reset-settings": "set ELECTRON_UPS_RESET_SETTINGS_ON_START=1&& set ELECTRON_UPS_DEBUG_LEVEL_ON_START=debug&& electron-forge start",
    "package": "electron-forge package",
    "package:win": "electron-forge package --platform=win32 --arch=x64",
    "make": "npm run make:nsis:full",
    "make:forge": "electron-forge make",
    "make:nsis": "electron-builder --prepackaged \"out/Easy UPS Client-win32-x64\" --win nsis --x64 --publish never",
    "make:nsis:full": "npm run package:win && npm run make:nsis",
    "publish": "electron-forge publish",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": {
    "name": "Owen Li",
    "email": "admin@iloli.ch"
  },
  "license": "GPL-2.0-only",
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/plugin-vite": "^7.11.1",
    "@electron/fuses": "^1.8.0",
    "@headlessui/react": "^2.2.9",
    "@tailwindcss/vite": "^4.2.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitejs/plugin-react": "^5.1.4",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.6",
    "electron": "40.6.0",
    "electron-builder": "^26.8.1",
    "eslint": "^9.31.0",
    "eslint-plugin-import": "^2.32.0",
    "lucide-react": "^0.575.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-i18next": "^16.5.4",
    "react-router-dom": "^7.13.0",
    "tailwindcss": "^4.2.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@duckdb/node-api": "^1.4.4-r.1",
    "electron-store": "^11.0.2",
    "i18next": "^25.8.13",
    "tslib": "^2.8.1",
    "zod": "^4.3.6"
  },
  "build": {
    "appId": "ch.iloli.easyupsclient",
    "productName": "Easy UPS Client",
    "directories": {
      "output": "out/nsis"
    },
    "win": {
      "icon": "assets/icons/app-icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "assets/icons/app-icon.ico",
      "uninstallerIcon": "assets/icons/app-icon.ico",
      "installerHeaderIcon": "assets/icons/app-icon.ico"
    }
  }
}
